{
  "name": "doc-server",
  "version": "1.0.0",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=node --target=node20 --outdir=dist",
    "dev": "nodemon --exec 'npm run build && node dist/index.cjs' --watch src -e ts"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@hocuspocus/server": "^2.8.1",
    "@hocuspocus/transformer": "^2.8.1",
    "database": "workspace:*",
    "tiptap-shared": "workspace:*",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.9",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "esbuild": "^0.19.11",
    "nodemon": "^3.0.2",
    "tsconfig": "workspace:*",
    "typescript": "^5.2.2"
  }
}
