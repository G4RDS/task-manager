generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Note {
  id             String   @id @default(uuid())
  title          String
  titleBlobUrl   String?
  contentBlobUrl String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
